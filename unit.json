{
  "listeners": {
    "*:80": {
      "pass": "routes",
      "forwarded": {
        "protocol": "X-Forwarded-Proto"
      }
    }
  },
  "routes": [
    {
      "match": {
        "uri": "!/index.php"
      },
      "action": {
        "share": "/var/www/html/public$uri",
        "fallback": {
          "pass": "applications/laravel"
        }
      }
    }
  ],
  "applications": {
    "laravel": {
      "type": "php",
      "user": "unit",
      "group": "unit",
      "processes": {
        "max": 10,
        "spare": 2
      },
      "root": "/var/www/html/public/",
      "script": "index.php"
    }
  }
}